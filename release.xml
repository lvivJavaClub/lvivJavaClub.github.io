---
layout: null
permalink: /release.xml
---
{% assign feed_url = page.url | absolute_url %}<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
<title>Release</title>
<!-- <subtitle>By {{ site.author.name }}</subtitle> -->
<!-- <link rel="alternate" type="text/html" href="{{ site.url }}" /> -->
<!-- <link rel="self" type="application/atom+xml" href="{{ feed_url }}" /> -->
<id>{{ feed_url }}</id>
<updated>{{ site.release[0].EntryPublished | date_to_xmlschema }}</updated>
<!-- <updated1>{{ site }} </updated1> -->
<rights>Copyright Â© {{ 'now' | date: "%Y" }}, Lviv JavaClub</rights>
  {% assign releases = site.releases | where_exp: "release", "release.draft != true" %} {% for release in releases limit: 50 %} 
  <entry>
    <title>{{ release.FeedTitle }}</title>
    <!-- <link rel="alternate" type="text/html" href="{{ release | absolute_url }}" /> -->
    <!-- <link rel="related" type="text/html" href="{{ release | absolute_url }}" /> -->
    <!-- <id>{{ release | absolute_url }}</id> -->
    <imageUrl>{{ release.EntryImageUrl }}</imageUrl>
    <published>{{ release.EntryPublished | date_to_xmlschema }}</published>
    <!-- <updated>{{ release.update_date | default: release.date | date_to_xmlschema }}</updated> -->
    <author>
      <name>Lviv JavaClub</name>
      <!-- <uri>{{ release.author.uri | default: site.url }}</uri> -->
    </author>
    <content type="html" xml:base="{{ site.url }}" xml:lang="en">
      <![CDATA[
      <div>
        {{ release.content }}
      </div>
      ]]>
    </content>
  </entry>{% endfor %}
</feed>
